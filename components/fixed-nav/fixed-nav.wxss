.mark {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 19999;
}

.fixed-nav {
    display: flex;
    position: fixed;
    height: 70rpx;
    border-radius: 35rpx;
    background-color: #f70;
    z-index: 20000;
}

.nav-item {
    height: 70rpx;
    width: 70rpx;
    
}

.nav-item_con{
    display: flex;
    transition: width .3s;
}

.nav-item-in:first-child {
    margin-left: 20rpx;
}

.nav-icon {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.inner-view {
    background-color: #f70;
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    box-sizing: border-box;
    padding: 18rpx;
    transform: rotateZ(45deg);
    border: 1rpx solid rgba(0, 0, 0, 0.1);
}

.inner-column {
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 50%;
}

.inner-item {
    width: 15rpx;
    height: 15rpx;
    border-radius: 4rpx;
    background-color: #fff;
}

.flex-end {
    align-items: flex-end;
}

width0 {
    width: 0;
}

.rotateZ45 {
    -webkit-animation-duration: 0.7s;
    animation-duration: 0.7s;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    -webkit-animation-name: rotateZ45;
    animation-name: rotateZ45;
}

@-webkit-keyframes rotateZ45 {
    from {
        -webkit-transform: rotateZ(0);
        transform: rotateZ(0);
    }

    to {
        -webkit-transform: rotateZ(-45deg);
        transform: rotateZ(-45deg);
    }
}

@keyframes rotateZ45 {
    from {
        -webkit-transform: rotateZ(0);
        transform: rotateZ(0);
    }

    to {
        -webkit-transform: rotateZ(-45deg);
        transform: rotateZ(-45deg);
    }
}

.rotateZ0 {
    -webkit-animation-duration: 0.3s;
    animation-duration: 0.3s;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    -webkit-animation-name: rotateZ0;
    animation-name: rotateZ0;
}

@-webkit-keyframes rotateZ0 {
    from {
        -webkit-transform: rotateZ(-45deg);
        transform: rotateZ(-45deg);
    }

    to {
        -webkit-transform: rotateZ(0);
        transform: rotateZ(0);
    }
}

@keyframes rotateZ0 {
    from {
        -webkit-transform: rotateZ(-45deg);
        transform: rotateZ(-45deg);
    }

    to {
        -webkit-transform: rotateZ(0);
        transform: rotateZ(0);
    }
}
