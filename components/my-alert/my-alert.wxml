<view class='my-warn type_{{type}}' wx:if="{{isClose}}" style='{{styleStr}}'>
        <text class='warn-text {{move?"fadeInRight":""}}' style="{{move?'transform: translate3d(100%, 0, 0);':''}}">{{title}}</text>
        <my-icon type='close' size='30' class='icon' color='{{iconColor||"#c0c4cc"}}' wx:if="{{closable}}" catchtap='hide' />
</view>