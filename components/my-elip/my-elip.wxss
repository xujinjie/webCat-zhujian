.parent-node {
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
}

.parent-node-right {
  display: flex;
  display: -webkit-flex;
}

.my-elip {
  box-sizing: border-box;
  color: #000;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
}

.my-elip-right {
  flex: 95;
}

.my-elip-bottom {
  width: 100%;
}

.icon {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  height: auto;
}

.icon-right {
  flex: 5;
  line-height: 200%;
}

.icon-bottom {
  width: 100%;
  height: 30rpx;
}

.isSlot{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
}